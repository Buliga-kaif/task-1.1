#include <stdio.h>

int main() {
double x, y, z; // Стороны кирпича
double r, s; // Стороны отверстия

// Ввод размеров кирпича с проверкой 
printf("Введите размеры кирпича (x, y, z): \n"); 
printf("x: "); 
while (scanf("%lf", &x) != 1 || x <= 0) { 
    printf("Ошибка ввода. Пожалуйста, введите положительное число для x: "); 
    while(getchar() != '\n'); // Очистка буфера 
} 
 
printf("y: "); 
while (scanf("%lf", &y) != 1 || y <= 0) { 
    printf("Ошибка ввода. Пожалуйста, введите положительное число для y: "); 
    while(getchar() != '\n'); // Очистка буфера 
} 
 
printf("z: "); 
while (scanf("%lf", &z) != 1 || z <= 0) { 
    printf("Ошибка ввода. Пожалуйста, введите положительное число для z: "); 
    while(getchar() != '\n'); // Очистка буфера 
} 

// Ввод размеров отверстия с проверкой 
printf("Введите размеры отверстия (r, s): \n"); 
printf("r: "); 
while (scanf("%lf", &r) != 1 || r <= 0) { 
    printf("Ошибка ввода. Пожалуйста, введите положительное число для r: "); 
    while(getchar() != '\n'); // Очистка буфера 
} 
 
printf("s: "); 
while (scanf("%lf", &s) != 1 || s <= 0) { 
    printf("Ошибка ввода. Пожалуйста, введите положительное число для s: "); 
    while(getchar() != '\n'); // Очистка буфера 
} 

// Проверка возможности прохождения кирпича через отверстие 
if ((x <= r && y <= s)  (x <= s && y <= r)  
    (x <= r && z <= s)  (x <= s && z <= r)  
    (y <= r && z <= s) || (y <= s && z <= r)) { 
    printf("Кирпич пройдет через отверстие.\n"); 
} else { 
    printf("Кирпич не пройдет через отверстие.\n"); 
} 

return 0; 
}
